      <key
            commandId="org.erlide.wrangler.refactoring.renameVariable"
            contextId="org.erlide.ui.erlangEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+R V">
      </key>
      <key
            commandId="org.erlide.wrangler.refactoring.renameFunction"
            contextId="org.erlide.ui.erlangEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+R F">
      </key>
      <key
            commandId="org.erlide.wrangler.refactoring.renameModule"
            contextId="org.erlide.ui.erlangEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+R M">
      </key>
      <key
            commandId="org.erlide.wrangler.refactoring.moveFunction"
            contextId="org.erlide.ui.erlangEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+V F">
      </key>
