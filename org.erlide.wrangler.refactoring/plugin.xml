            id="org.erlide.wrangler.refactoring.renameModule"
            name="Rename module">
      </command>
      <command
            categoryId="org.erlide.wrangler.refactoring"
            id="org.erlide.wrangler.refactoring.moveFunction"
            name="Move function">
      </command>
   </extension>
