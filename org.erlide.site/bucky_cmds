setpref targetPlatformPath=#{TARGET}

installJRE --location #{JAVA_HOME} --name erlide_jre
importpreferences -F org.erlide.site/_erl.epf -S instance

import #{WORKSPACE}/#{SITE_PRJ}/site.cquery
build -c -t 

perform -D target.os=* -D target.ws=* -D target.arch=* -D qualifier.replacement.*=generator:buildTimestamp -D generator.buildTimestamp.format=yyyyMMddHHmm -D cbi.include.source=true -D site.pack200=true #{SITE_PRJ}#site.p2

